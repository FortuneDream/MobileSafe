<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:yq="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        style="@style/title_one"
        android:text="设置中心" />

    <com.example.dell.mobilesafe.view.SettingItemView
        yq:titles="设置自动更新"
        yq:update_off="自动更新已经关闭"
        yq:update_on="自动更新已经开启"
        android:id="@+id/siv_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <com.example.dell.mobilesafe.view.SettingItemView
        yq:titles="设置黑名单拦截"
        android:id="@+id/siv_interrupt"
        yq:update_off="黑名单拦截已经关闭"
        yq:update_on="黑名单拦截已经开启"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <com.example.dell.mobilesafe.view.SettingItemView
        yq:titles="设置程序锁"
        yq:update_off="程序锁已经关闭"
        yq:update_on="程序锁已经开启"
        android:id="@+id/siv_lock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
