<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:yq="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroudWhite"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@color/titleGreen"
        android:gravity="center"
        android:text="设置中心"
        android:textColor="@color/textBlack"
        android:textSize="@dimen/title_size" />

    <com.example.dell.mobilesafe.view.SettingItemView
        yq:titles="设置自动更新"
        yq:update_off="自动更新已经关闭"
        yq:update_on="自动更新已经开启"
        android:id="@+id/siv_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
